<div class="mdl-grid">
  <div class="mdl-cell mdl-cell--12-col">
    <h1 class="center">2016</h1>
  </div>
</div>

<div class="mdl-grid">
  <% records.values.sort_by {|record| record["number"].to_i}.reverse.each do |record| %>
    <div class="mdl-cell mdl-cell--3-col">
      <div class="mdl-card mdl-shadow--2dp">
        <div class="mdl-card__title mdl-card--expand">
          <h2 class="mdl-card__title-text">
            <a name="<%= record["number"] %>" href="<%= "#{slugify(section)}/#{year}/#{record["number"]}" %>">
              <%= record["number"] %>
            </a>
          </h2>
        </div>
        <div class="mdl-card__supporting-text">
          <b>Proponente:</b><br /><%= record["author"] %><br />
          <b>Oggetto:</b><br /><%= record["subject"] %><br />
          <b>Data Inizio Pubb.:</b> <%= record["startingPublicationDate"] %><br />
          <b>Data Fine Pubb.:</b> <%= record["endingPublicationDate"] %><br />
          <b>Data Esec.:</b> <%= record["executionDate"] %>
        </div>
        <div class="mdl-card__actions mdl-card--border">
          <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" href="<%= "#{slugify(section)}/#{year}/#{record["number"]}" %>">
            Vedi documenti
            <span class="material-icons">launch</span>
          </a>
        </div>
      </div>
    </div>
  <% end %>
</div>
